<h1> <%= @cat.name %>!</h1>
</br>
<table style="border: 1px solid black;">
  <thead>
    <tr>
      <th>Attribute</th>
      <th>Value</th>
    </tr>
  <tr>
    <td>Age</td>
    <td> <%= @cat.age %> </td>
  </tr>
  <tr>
    <td>Birth Date</td>
    <td><%= @cat.birth_date %></td>
  </tr>
  <tr>
    <td>Color</td>
    <td><%= @cat.color %></td>
  </tr>
  <tr>
    <td>Sex</td>
    <td><%= @cat.sex %></td>
  </tr>
</table>
</br>
<%= link_to "Edit cat", edit_cat_url(@cat) %><br>
<%= link_to "Return to all cats", cats_url %>
